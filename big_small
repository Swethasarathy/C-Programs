#include<stdio.h>
void main()
{
int a,b,c,d,e;
printf("\nEnter three values a,b,c : ");
scanf("%d %d %d",&a,&b,&c);
d = (a > b) ?(a > c ? a : c) : (b > c ? b : c);
e = (a < b) ?(a < c ? a : c) : (b < c ? b : c);
printf("\n%d is the biggest element ", d);
printf("\n%d is the smallest element", e);
}
